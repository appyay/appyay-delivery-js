{
  "name": "appyay-delivery-js",
  "version": "0.0.1",
  "description": "JavaScript SDK for the Appyay Content Delivery API (node and browser).",
  "main": "/dist/appyay-delivery-js.node.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && node ./test/integration/*.js",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./dist/appyay-delivery-js.node.js)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richjava/appyay-delivery-js.git"
  },
  "keywords": [
    "appyay",
    "content",
    "delivery",
    "headless",
    "API",
    "Javascript",
    "SDK",
    "library",
    "npm"
  ],
  "author": "Richard Lovell <richjavalabs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/appyay/appyay-delivery-js/issues"
  },
  "homepage": "https://github.com/appyay/appyay-delivery-js#readme",
  "dependencies": {
    "axios": "^0.18.1",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "tape": "^4.11.0",
    "tape-promise": "^4.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
